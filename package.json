{
  "name": "tes-property-system",
  "version": "1.0.0",
  "description": "TES Property - Real Estate Inquiry Management System",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "server": "node server.js",
    "backend": "node server.js",
    "dev:full": "node server.js & vite",
    "dev:customer": "vite --config vite.customer.config.ts",
    "dev:admin": "vite --config vite.admin.config.ts",
    "dev:agent": "vite --config vite.agent.config.ts",
    "dev:all": "concurrently \"npm run backend\" \"npm run dev:customer\" \"npm run dev:admin\" \"npm run dev:agent\"",
    "build:customer": "vite build --config vite.customer.config.ts",
    "build:admin": "vite build --config vite.admin.config.ts",
    "build:agent": "vite build --config vite.agent.config.ts",
    "build:all": "npm run build:customer && npm run build:admin && npm run build:agent",
    "lint": "eslint src --ext ts",
    "format": "prettier --write src"
  },
  "dependencies": {
    "alpinejs": "^3.13.3",
    "browser-image-compression": "^2.0.2",
    "cors": "^2.8.5",
    "dompurify": "^3.3.1",
    "express": "^5.2.1",
    "joi": "^18.0.2",
    "jspdf": "^3.0.4",
    "papaparse": "^5.5.3",
    "workbox-window": "^7.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/papaparse": "^5.5.1",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.2.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.4.0",
    "terser": "^5.44.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.10"
  }
}
